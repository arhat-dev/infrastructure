  "regexManagers": [
    {
      "fileMatch": [".+.yaml$", ".+.yml$"],
      "matchStrings": [
        "- name: (?<repoName>.*?)/(?<depName>.*?)@(?<currentValue>.*?)\\s"
      ],
      "lookupNameTemplate": "{{{repoName}}}/{{{depName}}}",
      "datasourceTemplate": "helm",
      "versioningTemplate": "semver"
    }
  ],
  "kubernetes": {
    "fileMatch": [".+/manifests/.+\\.yaml$", ".+/manifests-custom/.+\\.yaml$"]
  },
